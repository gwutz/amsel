<?xml version="1.0"?>
<interface>
    <object class="GtkShortcutsWindow" id="shortcuts">
        <child>
            <object class="GtkShortcutsSection">
                <property name="visible">true</property>
                <property name="section-name">shortcuts</property>
                <child>
                    <object class="GtkShortcutsGroup">
                        <property name="visible">true</property>
                        <property name="title" translatable="no">General</property>
                        <child>
                            <object class="GtkShortcutsShortcut">
                                <property name="visible">true</property>
                                <property name="accelerator">&lt;ctrl&gt;A</property>
                                <property name="title" translatable="no">Add a new Feed</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkShortcutsShortcut">
                                <property name="visible">true</property>
                                <property name="accelerator">&lt;ctrl&gt;R</property>
                                <property name="title" translatable="no">Refresh all feeds</property>
                            </object>
                        </child>
                        <child>
                            <object class="GtkShortcutsShortcut">
                                <property name="visible">true</property>
                                <property name="accelerator">&lt;ctrl&gt;&lt;shift&gt;question</property>
                                <property name="title" translatable="no">Keyboard Shortcuts</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>
